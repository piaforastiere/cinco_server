{
  "name": "server",
  "version": "1.0.0",
  "description": "five powers server",
  "main": "index.js",
  "engines": {
    "node": "12.18.0",
    "npm": "6.14.4"
  },
  "scripts": {
    "start": "node index.js",
    "server": "nodemon ./index.js",
